{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "87900679-5162-4f8c-a537-fe716f600189"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-flow": {
      "projectType": "library",
      "root": "projects/ngx-flow",
      "sourceRoot": "projects/ngx-flow/src",
      "prefix": "flow",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ngx-flow/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-flow/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-flow/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/ngx-flow/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-flow/karma.conf.js",
            "codeCoverage": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-flow/**/*.ts",
              "projects/ngx-flow/**/*.html"
            ],
            "eslintConfig": "projects/ngx-flow/eslint.config.js"
          }
        }
      }
    },
    "ngx-flow-demo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ngx-flow-demo",
      "sourceRoot": "projects/ngx-flow-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/ngx-flow-demo",
            "index": "projects/ngx-flow-demo/src/index.html",
            "browser": "projects/ngx-flow-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ngx-flow-demo/tsconfig.app.json",
            "assets": [
              "projects/ngx-flow-demo/src/favicon.ico",
              "projects/ngx-flow-demo/src/assets"
            ],
            "styles": [
              "projects/ngx-flow-demo/src/styles.css"
            ],
            "scripts": [],
            "ssr": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngx-flow-demo:build:production"
            },
            "development": {
              "buildTarget": "ngx-flow-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "ngx-flow-demo:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "assets": [
              "projects/ngx-flow-demo/src/favicon.ico",
              "projects/ngx-flow-demo/src/assets"
            ],
            "styles": [
              "projects/ngx-flow-demo/src/styles.css"
            ],
            "scripts": [],
            "tsConfig": "projects/ngx-flow-demo/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-flow-demo/karma.conf.js",
            "codeCoverage": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-flow-demo/**/*.ts",
              "projects/ngx-flow-demo/**/*.html"
            ],
            "eslintConfig": "projects/ngx-flow-demo/eslint.config.js"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
